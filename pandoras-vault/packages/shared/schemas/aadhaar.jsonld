{
  "@context": [
    "https://www.w3.org/2018/credentials/v1",
    {
      "aadhaar": "https://pandoras-vault.example.com/schemas/aadhaar/v1#",
      "AadhaarCredential": "aadhaar:AadhaarCredential",
      "aadhaarNumber": "aadhaar:aadhaarNumber",
      "maskedAadhaarNumber": "aadhaar:maskedAadhaarNumber",
      "name": "aadhaar:name",
      "dob": "aadhaar:dob",
      "address": "aadhaar:address",
      "photoHash": "aadhaar:photoHash",
      "gender": "aadhaar:gender"
    }
  ],
  "type": "AadhaarCredential",
  "description": "Aadhaar-based verifiable credential schema for Pandora's Vault. Defines the structure and vocabulary for Aadhaar credentials issued by authorized issuers. Includes fields for Aadhaar number (masked for privacy), name, date of birth, address, photo hash, and gender. Complies with W3C Verifiable Credentials Data Model v1.1."
}
